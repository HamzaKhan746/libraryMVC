@{
    if(ViewBag.dashboard=="lib")
    {
        Layout = "~/Views/Shared/Libdashboard.cshtml";
    }
    else
    {
            Layout = "~/Views/Shared/Memberdashboard.cshtml";
    }
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" type="text/css" href="~/css/library.css" asp-append-version="true">
    <title>Message</title>
</head>
<body>
<h2>@ViewBag.Heading</h2>
<p style="text-align:center">@ViewBag.Message</p>
</body>
</html>