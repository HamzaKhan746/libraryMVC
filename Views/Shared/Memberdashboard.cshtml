<!DOCTYPE html>
<html>
<head>
	<title>@ViewData["Title"] - LibraryManagementSystem</title>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width">
	<link rel="stylesheet" type="text/css" href="~/css/dstyle.css" asp-append-version="true">
    
</head>
<body>
<header>
	<h1 style="padding-top:10px;">Library Management System</h1>
</header>
<nav>
	<ul class="one">
     <li><img src="~/css/img1.jpg" style="height:35px;"><a asp-area="" asp-controller="Book" asp-action="ViewBooks">View Books</a></li>
     <li><img src="~/css/img2.png" style="height:35px;"><a href="#">Books</a>
     	<ul>
     		<li><a asp-area="" asp-controller="SideMember" asp-action="IssueBookRequest"><b>IssueBook Request</b></a></li>
     		<li><a asp-area="" asp-controller="SideMember" asp-action="MReturnBook">Return Book</a></li>
     		</ul>
     </li>
     <li><img src="~/css/img5.jpg" style="height:35px;"><a href="#">Views</a>
         <ul>
         <li><a asp-area="" asp-controller="SideMember" asp-action="Mydetails">My Details</a></li>
           <li><a asp-area="" asp-controller="SideMember" asp-action="MyIssuedBooks">My Issued Books</a></li>
           <li><a asp-area="" asp-controller="SideMember" asp-action="MyReturnedBooks">My Return Books</a></li>
     	</ul>
         </li>
     <li><img src="~/css/img6.jpg" style="height:35px; width:32px;"><a href="#">Message</a></li>
     <li><img src="~/css/imgd.png" style="height:39px;">
         <ul>
         <li><a asp-area="" asp-controller="Home" asp-action="Index">Log out</a></li>
         </ul>
         </li>
         </ul>
        </nav>
        <div>@RenderBody()</div>
</body>
</html>
